{
  "expo": {
    "name": "CANSA-Dealer",
    "slug": "CANSA-Dealer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cansateam.cansashopdealer",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.cansateam.cansashopdealer",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  },
  "packagerOpts": {
    "config": "metro.config.js"
  }

}